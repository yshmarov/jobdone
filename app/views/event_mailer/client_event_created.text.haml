= t('.event_created')

- model_class = Job
= @event.services.pluck(:name).join(', ')
- model_class = Event
= model_class.human_attribute_name(:time) + ':'
= @event.starts_at

= t('.address')
= Tenant.current_tenant.name
= @event.workplace.location
= @event.workplace.location.phone_number
= @event.workplace.location.address_line

